        <div id="canvas">
            <div id="dashboard">
                <div id="dashbtn"> <div class="slider"></div> <span>Client</span> <span>Server</span> </div>


                <div id="client">
                    <div id="client-box">
                        <div class="title"><h1>Client</h1></div>
                        <div class="dash-box">
                            <h4>Infos</h4>
                            <div class="info-col1">
                                <p>Name : <span><%= hostname %></span></p>
                                <p>Your ip : <span class="localIp <%= localip === 'Connection error' ? 'no-blur' : 'blurrable' %>"><%= localip%></span></p>
                                <p>Using mfs : <span><%= mfsVersion %></span></p>
                            </div>
                            <div class="info-coll2">
                                <p>Status : <span id="clientst" class="status">Not connected</span></p>
                                <p>Connected to : <span id="host-target">None</span></p>
                                <p>Listening on port : <span id="list-port"></span></p>
                            
                            </div>
                        </div>
                    </div>
                        <div class="connection-box">
                            <form id="connect-form" method="post" action="/connect">
                            <input type="text" name="host" placeholder="Host ip" id="host-ip-input" required>
                            <input type="text" name="code" id="host-code-input" placeholder="Connection code" required>
                            <button id="connect-btn" type="submit">Connect</button>
                        </form>
                    </div>
                </div>
                
<!--###########################################################################-->

                <div id="host">
                    <div id="host-box">
                        <div class="title"><h1>Server</h1></div>
                        <div class="dash-box">
                            <h4>Infos</h4>
                            <div class="info-col1">
                                <p>Name : <span><%= hostname %></span></p>
                                <p>Your ip : <span class="localIp"><%= localip%></span></p>
                                <p>Using mfs : <span><%= mfsVersion %></span></p>
                            </div>
                            <div class="info-coll2">
                                <p>Status : <span id="serverst" class="status">Not running</span></p>
                                <p>Connected to : <span id="host-target">None</span></p>
                                <p>Listening on port : <span id="list-port"></span></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


<!------------------------------------------------------------------------->


    </div>
        <span id="overlay"></span>
        <div id="settings">
            <div class="settings-top">
                <h1>Settings</h1>
            </div>

            <div class="settings-middle" id="params">

            </div>

            <div class="settings-bottom">
                <button id="saveSettings">Close</button>
            </div>
        </div>
        <span id="toast">
            <span class="icon"></span>     
            <span class="message"> 
                <h6>Toast</h6>
                <a>Some explication</a>
            </span>
